{
  "name": "mwc-demos",
  "private": true,
  "description": "Demos using MyUW Web Components in various ways",
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean copy build:rollup",
    "build:rollup": "rollup -c",
    "watch": "run-p watch:copy watch:rollup",
    "watch:rollup": "rollup -c -w",
    "watch:copy": "chokidar 'src/*!(.js)' -c 'npm run copy'",
    "copy": "copyfiles -u 1 -e src/*.js src/* dist",
    "serve": "live-server dist/",
    "start": "run-p watch serve",
    "prepare": "npm run build",
    "pages": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidmsibley/mwc-demos.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/davidmsibley/mwc-demos/issues"
  },
  "homepage": "https://github.com/davidmsibley/mwc-demos#readme",
  "devDependencies": {
    "@myuw-web-components/myuw-app-bar": "^1.3.1",
    "@myuw-web-components/myuw-app-styles": "^1.1.0",
    "@myuw-web-components/myuw-drawer": "^1.0.0",
    "@myuw-web-components/myuw-help": "^1.1.0",
    "@myuw-web-components/myuw-profile": "^1.1.0",
    "chokidar-cli": "^1.2.0",
    "copyfiles": "^2.0.0",
    "gh-pages": "^1.2.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.4",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-html-entry": "^0.2.0",
    "rollup-plugin-minify-es": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "tota11y": "^0.1.6"
  },
  "dependencies": {},
  "files": [
    "dist"
  ]
}
